<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<libs>
		<Miaox>
			<plugin>Standart</plugin>
			<deploy>
				<type><![CDATA[git]]></type>
				<tag><![CDATA[master]]></tag>
				<src>http://github.com/TheRatG/miaox.git</src>
				<dst>${config.paths.libs}/miaox/${config.libs.Miaox.deploy.tag}</dst>
			</deploy>
			<configure>
				<depends>
					<item>PHPUnit</item>					
					<item>Miao</item>
				</depends>
				<dirs>
					<item>
						<dir>${config.libs.Miaox.deploy.dst}/data</dir>
					</item>
				</dirs>
			</configure>
			<link></link>
			<modules>
				<Aop>
					<enabled>true</enabled>
					<cache_dir>${config.paths.shared}/aop_cache_dir</cache_dir>
					<recompile>0</recompile>
					<encoding>utf-8</encoding>
					<compact>1</compact>
				</Aop>
			</modules>
		</Miaox>
	</libs>
</config>